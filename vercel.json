{
    "functions": {
        "api/index.php": {
          "runtime": "vercel-php@0.6.0"
        },
        "api/api.php": {
          "runtime": "vercel-php@0.6.0"
        }
    },
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.php"
      }
    ]
  }