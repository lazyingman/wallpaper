{
  "functions": {
      "api/**.*": {
        "runtime": "vercel-php@0.6.0"
      }
  },
  "routes": [
      {
        "src": "/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/css/(.*)",
        "dest": "/api/css/$1"
      },
      {
        "src": "/image/(.*)",
        "dest": "/api/image/$1"
      },
      {
        "src": "/js/(.*)",
        "dest": "/api/js/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
    ]
}