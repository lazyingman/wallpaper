{
    "functions": {
      "api/index.php": {
        "runtime": "vercel-php@0.6.0"
      },
      "api/api.php": {
        "runtime": "vercel-php@0.6.0"
      }
    },
    "routes": [
        {
          "src": "/*.php",
          "dest": "/api/*.php"
        },
        {
          "src": "/*.js",
          "dest": "/api/js/*.js"
        },
        {
            "src": "/*.css",
            "dest": "/api/css/*.js"
        }
      ]
}